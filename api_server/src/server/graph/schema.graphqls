# GraphQL schema example
#
# https://gqlgen.com/getting-started/

#union Field = String | Int

scalar Map

type Meta {
  id: ID!
  text: String!
  customValues: Map
}

input NewMeta {
  text: String!
  customValues: Map
}

type Mutation {
  createMeta(input: NewMeta!): Meta!
}

type Query {
  metas (filter: Map): [Meta]
}